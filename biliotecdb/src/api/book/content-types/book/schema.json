{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "id_libro": {
      "type": "string",
      "required": true
    },
    "titulo": {
      "type": "string",
      "required": true
    },
    "autor": {
      "type": "string",
      "required": true
    },
    "clasificacion": {
      "type": "string"
    },
    "loans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan",
      "mappedBy": "book"
    },
    "inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory",
      "mappedBy": "book"
    }
  }
}
